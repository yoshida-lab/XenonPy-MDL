  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.15"
  },
  "scripts": {
    "dev": "concurrently -r \"npx next\" \"npx graphql-codegen --watch\"",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "yarn build:nexus-typegen && next build",
    "start": "next start",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:reset": "prisma migrate reset --preview-feature",
    "db:push": "prisma db push --preview-feature",
    "gen:prisma": "prisma generate",
    "gen:hook": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-urql": "^2.0.5",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/micro-cors": "^0.1.1",
    "@types/minio": "^7.0.7",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-alloy": "^3.10.0",
    "prettier": "^2.2.1",
    "prisma": "2.18.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "@prisma/client": "2.18.0",
    "apollo-server-micro": "^2.22.2",
    "bcrypt": "^5.0.1",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.9.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
    "micro-cors": "^0.1.1",
    "minio": "^7.0.18",
    "nanoid": "^3.1.22",
    "next": "^10.1.3",
    "next-auth": "^3.13.3",
    "next-urql": "^3.0.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "0.32.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.2.3",
    "urql": "^2.0.2"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0"
  }
}
